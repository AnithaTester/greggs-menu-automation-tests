<?xml version="1.0" encoding="UTF-8"?><suite name="Cross Browser Tests" parallel="tests" thread-count="3">    <test name="Chrome Tests">        <parameter name="browser" value="chrome"/>        <packages>            <package name="com.greggs.tests.specs.*"/>        </packages>    </test>    <test name="Firefox Tests">        <parameter name="browser" value="firefox"/>        <packages>            <package name="com.greggs.tests.specs.*"/>        </packages>    </test>    <test name="Safari Tests">        <parameter name="browser" value="safari"/>        <packages>            <package name="com.greggs.tests.specs.*"/>        </packages>    </test></suite>